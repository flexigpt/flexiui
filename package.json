{
	"name": "flexigpt",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"main": "agentts/build/main.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/flexigpt/flexiui.git"
	},
	"author": "Pankaj Pipada <ppipada@gmail.com>",
	"description": "FlexiGPT - An AI application platform",
	"scripts": {
		"lint:frontend": "pnpm -F frontend run lint",
		"lintfix:frontend": "pnpm -F frontend run lint -- --fix",
		"fmt": "prettier -c . --ignore-path ./.prettierignore",
		"fmt:fix": "prettier -w . --ignore-path ./.prettierignore",
		"clean:frontend": "pnpm -F frontend run clean",
		"copy:icon": "shx mkdir -p build && shx cp frontend/public/icon.png build/appicon.png",
		"build:frontend": "pnpm -F frontend run build:wails && pnpm run copy:icon",
		"build:linux": "cd ./cmd/agentgo && pnpm run build:frontend && wails build -m -clean -s -skipbindings -tags webkit2_41 -platform linux/amd64",
		"build:mac": "cd ./cmd/agentgo && pnpm run build:frontend && wails build -m -clean -s -skipbindings -platform darwin/universal",
		"build:win": "cd ./cmd/agentgo && pnpm run build:frontend && wails build -m -clean -s -skipbindings -nsis -platform windows/amd64",
		"pack:flatpak": "gh act -W './.github/workflows/flatpak.yml' --artifact-server-path ./out",
		"pack:nsis": "gh act -W './.github/workflows/nsispak.yml' --artifact-server-path ./out",
		"pack:dmg": "gh act -W './.github/workflows/dmgpak.yml' --artifact-server-path ./out",
		"build:withbindings": "cd ./cmd/agentgo && pnpm run build:frontend && wails build -devtools -m -clean -v 2",
		"run:watch": "cd ./cmd/agentgo &&  wails dev -s -v 2 -tags webkit2_41",
		"run:gobackend": "./scripts/run_backend.sh",
		"knip": "knip --tags=-lintignore"
	},
	"devDependencies": {
		"@types/node": "^20.17.6",
		"@typescript-eslint/eslint-plugin": "^8.13.0",
		"@typescript-eslint/parser": "^8.13.0",
		"eslint": "^8.57.1",
		"eslint-config-next": "14.2.11",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-prettier": "^5.2.1",
		"knip": "^5.43.6",
		"prettier": "^3.3.3",
		"shx": "^0.3.4",
		"typescript": "^5.6.3"
	},
	"knip": {
		"ignore": [
			"pkg/mcpsdk/external/schema_24_12_2024.ts",
			"frontend/app/backendapibase/wailsjs/runtime/runtime.js",
			"frontend/app/models/aiprovidermodel/chat_types.ts",
			"frontend/app/backendapihelper/conversation_cache.ts"
		],
		"ignoreBinaries": [
			"wails",
			"gh"
		]
	},
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": true,
		"jsxSingleQuote": false,
		"printWidth": 120,
		"proseWrap": "preserve",
		"quoteProps": "as-needed",
		"requirePragma": false,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5"
	},
	"packageManager": "pnpm@10.2.0+sha512.0d27364e0139c6aadeed65ada153135e0ca96c8da42123bd50047f961339dc7a758fc2e944b428f52be570d1bd3372455c1c65fa2e7aa0bfbf931190f9552001"
}
