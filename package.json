{
	"name": "flexiui",
	"version": "0.1.0",
	"main": "backend/build/main.js",
	"private": true,
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/flexigpt/flexiui.git"
	},
	"author": "Pankaj Pipada <ppipada@gmail.com>",
	"description": "UI for the flexigpt ecosystem",
	"scripts": {
		"frontend:start": "next start frontend",
		"frontend:dev": "next dev frontend",
		"frontend:build": "next build frontend",
		"frontend:lint": "next lint frontend",
		"backend:build": "tsc -p backend && tsc-alias -p ./backend/tsconfig.json",
		"backend:lint": "eslint backend",
		"lint": "prettier -c . && npm run backend:lint && npm run frontend:lint",
		"lint:fix": "prettier -w .",
		"boot": "npm run backend:build && electron .",
		"start": "concurrently -k \"npm run frontend:dev\" \"wait-on tcp:127.0.0.1:3000 && npm run boot\"",
		"pre-build": "npm run frontend:build && npm run backend:build",
		"build": "npm run pre-build && electron-builder",
		"postinstall": "electron-builder install-app-deps",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"electron-is-dev": "^3.0.1",
		"electron-log": "^5.1.7",
		"electron-updater": "6.3.1",
		"lowdb": "^7.0.1",
		"next": "14.2.5",
		"next-themes": "^0.3.0",
		"react": "^18",
		"react-dom": "^18",
		"react-icons": "^5.2.1",
		"react-markdown": "^9.0.1",
		"react-syntax-highlighter": "^15.5.0",
		"uuid": "^10.0.0"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "1",
		"@storybook/addon-essentials": "^8.2.6",
		"@storybook/addon-interactions": "^8.2.6",
		"@storybook/addon-links": "^8.2.6",
		"@storybook/addon-mdx-gfm": "^8.2.6",
		"@storybook/addon-onboarding": "^8.2.6",
		"@storybook/blocks": "^8.2.6",
		"@storybook/nextjs": "^8.2.6",
		"@storybook/react": "^8.2.6",
		"@storybook/test": "8.2.6",
		"@svgr/webpack": "^8.1.0",
		"@types/babel__generator": "^7.6.8",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@types/react-syntax-highlighter": "^15.5.13",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^7.17.0",
		"@typescript-eslint/parser": "^7.17.0",
		"concurrently": "^8.2.2",
		"daisyui": "^4.12.10",
		"electron": "^31.3.0",
		"electron-builder": "^24.13.3",
		"electron-devtools-installer": "^3.2.0",
		"eslint": "^8",
		"eslint-config-next": "14.2.5",
		"eslint-plugin-storybook": "^0.8.0",
		"postcss": "^8",
		"prettier": "^3.3.3",
		"storybook": "^8.2.6",
		"tailwindcss": "^3.4.7",
		"tsc-alias": "^1.8.10",
		"tsconfig-paths-webpack-plugin": "^4.1.0",
		"typescript": "^5",
		"wait-on": "^7.2.0"
	}
}
