{
	"name": "flexiui",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"main": "packages/flexiuiagent/build/main.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/flexigpt/flexiui.git"
	},
	"author": "Pankaj Pipada <ppipada@gmail.com>",
	"description": "UI for the flexigpt ecosystem",
	"scripts": {
		"lint": "lerna run lint",
		"lint:fix": "lerna run lint -- --fix",
		"fmt": "prettier -c .",
		"fmt:fix": "prettier -w .",
		"clean": "lerna run clean",
		"pre-pack": "lerna run build --ignore frontend --ignore flexiuiagent --stream --sort && lerna run build:electron --scope frontend && lerna run build --scope flexiuiagent",
		"buildpack": "lerna run clean && pnpm run pre-pack && lerna run pack --scope flexiuiagent",
		"boot": "lerna run build --scope flexiuiagent && electron .",
		"dev": "lerna run build --ignore frontend --ignore flexiuiagent --stream --sort && concurrently -k \"lerna run dev:electron --scope frontend\" \"wait-on tcp:127.0.0.1:3000 && pnpm run boot\""
	},
	"dependencies": {},
	"devDependencies": {
		"electron": "31.3.0",
		"@typescript-eslint/eslint-plugin": "^8.3.0",
		"@typescript-eslint/parser": "^8.3.0",
		"concurrently": "^9.0.1",
		"eslint": "^9",
		"eslint-config-next": "14.2.15",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.2.1",
		"lerna": "^8.1.7",
		"prettier": "^3.3.3",
		"rimraf": "^6.0.1",
		"tsc-alias": "^1.8.10",
		"tsconfig-paths-webpack-plugin": "^4.1.0",
		"typescript": "^5",
		"wait-on": "^8.0.0"
	}
}
