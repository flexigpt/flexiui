{
	"name": "flexiui",
	"version": "0.1.0",
	"main": "backend/build/main.js",
	"private": true,
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/flexigpt/flexiui.git"
	},
	"author": "Pankaj Pipada <ppipada@gmail.com>",
	"description": "UI for the flexigpt ecosystem",
	"scripts": {
		"frontend:start": "next start frontend",
		"frontend:dev": "next dev frontend",
		"frontend:build": "next build frontend",
		"frontend:lint": "cd frontend; eslint . --ext .ts,.tsx; next lint .;",
		"backend:build": "tsc -p backend && tsc-alias -p ./backend/tsconfig.json",
		"backend:lint": "cd backend; eslint . --ext .ts",
		"sharedpkg:lint": "cd sharedpkg; eslint . --ext .ts",
		"sharedpkg:build": "tsc -p sharedpkg && tsc-alias -p ./sharedpkg/tsconfig.json",
		"lint": "npm run sharedpkg:lint && npm run backend:lint && npm run frontend:lint",
		"fmt": "prettier -c .",
		"fmt:fix": "prettier -w .",
		"boot": "npm run backend:build && electron .",
		"start": "concurrently -k \"npm run frontend:dev\" \"wait-on tcp:127.0.0.1:3000 && npm run boot\"",
		"pre-build": "npm run sharedpkg:build && npm run frontend:build && npm run backend:build",
		"build": "npm run pre-build && electron-builder",
		"postinstall": "electron-builder install-app-deps"
	},
	"dependencies": {
		"electron-is-dev": "^3.0.1",
		"electron-log": "^5.1.7",
		"electron-updater": "6.3.1",
		"lowdb": "^7.0.1",
		"next": "14.2.5",
		"next-themes": "^0.3.0",
		"react": "^18",
		"react-dom": "^18",
		"react-icons": "^5.2.1",
		"react-markdown": "^9.0.1",
		"react-syntax-highlighter": "^15.5.0",
		"rehype-react": "^8.0.0",
		"remark-gfm": "^4.0.0",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.0",
		"sharedpkg": "1.0.0",
		"uuid": "^10.0.0"
	},
	"devDependencies": {
		"@svgr/webpack": "^8.1.0",
		"@types/babel__generator": "^7.6.8",
		"@types/http-cache-semantics": "^4.0.4",
		"@types/ms": "^0.7.34",
		"@types/node": "^20",
		"@types/prop-types": "^15.7.12",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@types/react-syntax-highlighter": "^15.5.13",
		"@types/unist": "^3.0.2",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^7.17.0",
		"@typescript-eslint/parser": "^7.17.0",
		"concurrently": "^8.2.2",
		"daisyui": "^4.12.10",
		"electron": "^31.3.0",
		"electron-builder": "^24.13.3",
		"electron-devtools-installer": "^3.2.0",
		"eslint": "^8.57.0",
		"eslint-config-next": "14.2.5",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.2.1",
		"postcss": "^8",
		"prettier": "^3.3.3",
		"tailwindcss": "^3.4.7",
		"tsc-alias": "^1.8.10",
		"tsconfig-paths-webpack-plugin": "^4.1.0",
		"typescript": "^5",
		"wait-on": "^7.2.0"
	},
	"optionalDependencies": {
		"@next/swc-darwin-arm64": "14.2.5",
		"@next/swc-darwin-x64": "14.2.5",
		"@next/swc-linux-arm64-gnu": "14.2.5",
		"@next/swc-linux-arm64-musl": "14.2.5",
		"@next/swc-linux-x64-gnu": "14.2.5",
		"@next/swc-linux-x64-musl": "14.2.5",
		"@next/swc-win32-arm64-msvc": "14.2.5",
		"@next/swc-win32-ia32-msvc": "14.2.5",
		"@next/swc-win32-x64-msvc": "14.2.5"
	},
	"workspaces": [
		"sharedpkg"
	]
}
