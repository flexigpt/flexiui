{
	"name": "frontend",
	"version": "0.0.6",
	"description": "Frontend for FlexiGPT",
	"type": "module",
	"private": true,
	"scripts": {
		"lint": "eslint .; knip --tags=-lintignore",
		"lint:fix": "eslint --fix .",
		"fmt": "prettier -c . --ignore-path ../.prettierignore",
		"fmt:fix": "prettier -w . --ignore-path ../.prettierignore",
		"clean": "rimraf ./build",
		"run:wailsdev": "pnpm run build:wails && cp .env.dev .env && cross-env VITE_PLATFORM=wails react-router dev",
		"build:wails": "rimraf ./build && cp .env.prod .env && cross-env VITE_PLATFORM=wails react-router build",
		"typecheck": "react-router typegen && tsc"
	},
	"keywords": [],
	"author": "ppipada",
	"dependencies": {
		"@fontsource-variable/inter": "^5.2.5",
		"@react-router/node": "^7.5.0",
		"isbot": "^5",
		"katex": "^0.16.11",
		"mermaid": "^11.6.0",
		"next-themes": "^0.4.6",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-icons": "^5.3.0",
		"react-markdown": "^9.0.1",
		"react-router": "^7.5.0",
		"react-syntax-highlighter": "^15.6.1",
		"rehype-katex": "^7.0.1",
		"remark-gemoji": "^8.0.0",
		"remark-gfm": "^4.0.0",
		"remark-math": "^6.0.0",
		"remark-supersub": "^1.0.0",
		"sprintf-js": "^1.1.3",
		"uuid": "^11.0.3"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.23.0",
		"@nabla/vite-plugin-eslint": "^2.0.5",
		"@react-router/dev": "^7.4.1",
		"@tailwindcss/vite": "^4.1.1",
		"@types/node": "^20.17.6",
		"@types/react": "^19.1.0",
		"@types/react-dom": "^19.1.0",
		"@types/react-syntax-highlighter": "^15.5.13",
		"@types/sprintf-js": "^1.1.4",
		"@types/uuid": "^10.0.0",
		"cross-env": "^7.0.3",
		"daisyui": "^5.0.12",
		"eslint": "^9.23.0",
		"eslint-plugin-prettier": "^5.2.6",
		"eslint-plugin-react": "^7.37.4",
		"globals": "^16.0.0",
		"jiti": "^2.4.2",
		"knip": "^5.46.5",
		"prettier": "^3.5.3",
		"react-router-devtools": "^1.1.0",
		"rimraf": "^6.0.1",
		"tailwindcss": "^4.1.1",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.29.0",
		"vite": "^6.2.4",
		"vite-tsconfig-paths": "^5.1.3"
	},
	"knip": {
		"ignore": [
			"app/backendapibase/wailsjs/runtime/runtime.js",
			"app/models/aiprovidermodel.ts",
			"app/backendapihelper/conversation_cache.ts"
		],
		"ignoreBinaries": [],
		"ignoreDependencies": [
			"@react-router/node",
			"isbot",
			"daisyui",
			"react-router-devtools",
			"tailwindcss",
			"@fontsource-variable/inter"
		]
	},
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": true,
		"jsxSingleQuote": false,
		"printWidth": 120,
		"proseWrap": "preserve",
		"quoteProps": "as-needed",
		"requirePragma": false,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5"
	}
}
